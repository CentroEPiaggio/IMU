 553          uint8 ReadControlRegister(uint8 address){
 554   1                      
 555   1                      uint8 controlreg;
 556   1                      
 557   1                      SPI_IMU_WriteByte(MPU9250_RCR | address);
 558   1                  SPI_IMU_WriteByte(0x00);
 559   1                      while(!( SPI_IMU_ReadStatus() & SPI_IMU_STS_SPI_DONE));
 560   1                      controlreg = SPI_IMU_ReadByte(); //real data
 561   1                      return controlreg;
 562   1      }
