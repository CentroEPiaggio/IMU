 512          /********************************** *********************************************
 513          * Function Name: Write Control Register
 514          *********************************************************************************/
 515          void WriteControlRegister(uint8 address, uint8 dta){
 516   1              
 517   1              SPI_IMU_ClearRxBuffer();
 518   1              SPI_IMU_ClearTxBuffer();
 519   1              SPI_IMU_ClearFIFO();
 520   1              SPI_IMU_WriteByte(MPU9250_WCR | address);
 521   1              while(!( SPI_IMU_ReadStatus() & SPI_IMU_STS_TX_FIFO_EMPTY));            
 522   1              SPI_IMU_WriteByte(dta);
 523   1              while(!( SPI_IMU_ReadStatus() & SPI_IMU_STS_TX_FIFO_EMPTY));
 524   1      }
 525          
 526          /*******************************************************************************
 527          * Function Name: Read Control Register
 528          *********************************************************************************/
 529          uint8 ReadControlRegister(uint8 address){
 530   1                      
 531   1                      uint8 controlreg;
 532   1                      
 533   1                      SPI_IMU_WriteByte(MPU9250_RCR | address);
 534   1                  SPI_IMU_WriteByte(0x00);
 535   1                      while(!( SPI_IMU_ReadStatus() & SPI_IMU_STS_SPI_DONE));
 536   1                      controlreg = SPI_IMU_ReadByte(); //real data
 537   1                      return controlreg;
 538   1      }
