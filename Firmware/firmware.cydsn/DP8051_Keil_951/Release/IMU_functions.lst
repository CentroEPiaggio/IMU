 539          void WriteControlRegister(uint8 address, uint8 dta){
 540   1              
 541   1              SPI_IMU_ClearRxBuffer();
 542   1              SPI_IMU_ClearTxBuffer();
 543   1              SPI_IMU_ClearFIFO();
 544   1              SPI_IMU_WriteByte(MPU9250_WCR | address);
 545   1              while(!( SPI_IMU_ReadStatus() & SPI_IMU_STS_TX_FIFO_EMPTY));            
 546   1              SPI_IMU_WriteByte(dta);
 547   1              while(!( SPI_IMU_ReadStatus() & SPI_IMU_STS_TX_FIFO_EMPTY));
 548   1      }
